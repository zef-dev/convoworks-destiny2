{
    "name" : "zef-dev/convoworks-destiny2",
    "type" : "library",
    "description" : "Convoworks package that aid with the Bungie.net API, used for Destiny 2",
    "homepage" : "https://github.com/zef-dev/convoworks-destiny2",
    "keywords" : [
        "convoworks",
        "bungie",
        "destiny"
    ],
    "license" : "MIT",
    "require" : {
        "php" : "^7.2",
		"psr/log" : "^1.1.0",
        "zef-dev/convoworks-core" : "^0.18"
    },
    "autoload" : {
        "psr-4" : {
            "Convo\\Api\\": "src/Convo/Api",
            "Convo\\Enums\\": "src/Convo/Enums",
            "Convo\\Service\\": "src/Convo/Service",
            "Convo\\Pckg\\Destiny\\" : "src/Convo/Pckg/Destiny"
        }
    },
    "config" : {
        "platform" : {
            "php" : "7.2"
        },
        "sort-packages" : true
    },
    "minimum-stability" : "beta"
}